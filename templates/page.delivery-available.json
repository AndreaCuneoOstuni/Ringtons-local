    "ea107a6f-b178-4d12-8fcd-4b9b5abf5cc8": {
      "type": "secondary-image-with-text",
      "settings": {
        "image": "shopify:\/\/shop_images\/Group-153.png",
        "layout": "image_first",
        "title": "We'll be in touch soon!",
        "title_capitalization": "none",
        "sub-title": "",
        "pre-text": "",
        "text": "<p>Our Customer Care team is checking for your nearest available van, packed with our unique tea blends and much more. We’ll be in touch shortly.<\/p>\n\n<p>Please keep an eye out for a call from our Customer Care team on 0800 052 2440.<\/p>",
        "sub-text": "",
        "desktop_content_alignment": "left",
        "color_scheme": "background-1",
        "mobile_content_alignment": "center",
        "padding_top": 36,
        "padding_bottom": 0
